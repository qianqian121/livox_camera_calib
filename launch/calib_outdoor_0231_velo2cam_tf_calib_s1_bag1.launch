<launch>
  <node 
    pkg="livox_camera_calib"
    type="lidar_camera_calib"
    name="lidar_camera_calib"
    args="$(find livox_camera_calib)/config/camera_0231_002.yaml
        $(find livox_camera_calib)/config/config_outdoor_robosense-m1_ar0231_002_tf_calib.yaml
        /mnt/data/ros/outdoor_20220102/small_pitch_with_trees_not_ideal/2022-01-02-22-09-55.bag
        $(find livox_camera_calib)/result_outdoor_20220102_s1_bag1"
    output="screen"
  />
  <node 
    pkg="rviz"
    type="rviz"
    name="rviz"
    args="-d $(find livox_camera_calib)/rviz_cfg/calib.rviz"
  />
</launch>
