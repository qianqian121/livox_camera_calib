<launch>
  <node 
    pkg="livox_camera_calib"
    type="lidar_camera_calib_online"
    name="lidar_camera_calib_online"
    args="$(find livox_camera_calib)/config/camera_0231_002.yaml
        $(find livox_camera_calib)/config/config_outdoor_robosense-m1_ar0231_002_tf.yaml
        /mnt/data/ros/outdoor/calib4/2021-12-23-01-00-30.bag
        $(find livox_camera_calib)/result_002_outdoor_calib4_bag1"
    output="screen"
  />
  <node 
    pkg="rviz"
    type="rviz"
    name="rviz"
    args="-d $(find livox_camera_calib)/rviz_cfg/calib_indoor.rviz"
  />
</launch>
