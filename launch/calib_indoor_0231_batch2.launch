<launch>
  <node 
    pkg="livox_camera_calib"
    type="lidar_camera_calib"
    name="lidar_camera_calib"
    args="$(find livox_camera_calib)/config/camera_0231_002.yaml
        $(find livox_camera_calib)/config/config_indoor_robosense-m1_ar0231_002.yaml
        /mnt/data/ros/livox_camera_calib/robosense-m1_ar0231/20211209_batch2_office/2021-12-10-01-12-56.bag
        $(find livox_camera_calib)/result_002"
    output="screen"
  />
  <node 
    pkg="rviz"
    type="rviz"
    name="rviz"
    args="-d $(find livox_camera_calib)/rviz_cfg/calib_indoor.rviz"
  />
</launch>
