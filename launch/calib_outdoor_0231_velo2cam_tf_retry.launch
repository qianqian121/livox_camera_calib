<launch>
  <node 
    pkg="livox_camera_calib"
    type="lidar_camera_calib"
    name="lidar_camera_calib"
    args="$(find livox_camera_calib)/config/camera_0231_002.yaml
        $(find livox_camera_calib)/config/config_indoor_robosense-m1_ar0231_002_tf.yaml
        /mnt/data/ros/outdoor/calib1/2021-12-23-00-45-38.bag
        $(find livox_camera_calib)/result_002_outdoor_calib1_bag6_retry"
    output="screen"
  />
  <node 
    pkg="rviz"
    type="rviz"
    name="rviz"
    args="-d $(find livox_camera_calib)/rviz_cfg/calib_indoor.rviz"
  />
</launch>
